<%= turbo_frame_tag "task_checkbox" do %>
  <%= form_with(model: task, url: complete_task_path(task), method: :put) do |f| %>
    <%= f.check_box(:completed, checked: task.completed, data: { controller: "task", action: "task#updateComplete" }, class: "w-4 h-4 text-blue-600 bg-gray-100 border-gray-300 rounded focus:ring-blue-500 dark:focus:ring-blue-600 dark:ring-offset-gray-800 focus:ring-2 dark:bg-gray-700 dark:border-gray-600") %> 
  <% end %>
<% end %>
